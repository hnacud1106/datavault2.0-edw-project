{
  "name": "clickhouse-source-connector",
  "config": {
    "connector.class": "io.debezium.connector.jdbc.JdbcSourceConnector",
    "connection.url": "jdbc:clickhouse://clickhouse-source:8123/staging",
    "connection.username": "default",
    "connection.password": "source_password_123",
    "topic.prefix": "edw_cdc",
    "table.include.list": "staging.product_changes_log",
    "mode": "timestamp",
    "timestamp.column.name": "change_timestamp",
    "validate.non.null": false,
    "poll.interval.ms": 5000,
    "batch.max.rows": 1000,
    "topic.creation.enable": "true",
    "topic.creation.default.replication.factor": 1,
    "topic.creation.default.partitions": 3,
    "topic.creation.default.cleanup.policy": "compact"
  }
}
